{
  react: {

    html: '''
    <div class="example-chart"></div>
    <div class="example-controls">
      <label for="yGrid">
        <input id="yGrid" type="checkbox">Y grid
      </label>
      <label for="xGrid">
        <input id="xGrid" type="checkbox">X grid
      </label>
    </div>
  '''
  js: '''
    var chart = React.createElement(LineChart, {data: data, options: options});
    var element = document.getElementsByClassName('example-chart')[testIndex || 0]
    var comp = ReactDOM.render(chart, element);

    document.getElementById('yGrid').addEventListener('change', function(event) {
      options.grid.y = event.target.checked;
      comp.updateAll();
    });

    document.getElementById('xGrid').addEventListener('change', function(event) {
      options.grid.x = event.target.checked;
      comp.updateAll();
    });

  '''

    }

angular: {

    html: '''
      <div class="example-chart">
        <linechart data="data" options="options"></linechart>
      </div>
      <div class="example-controls">
        <label for="yGrid">
          <input id="yGrid" type="checkbox" ng-model="options.grid.y">Y grid
        </label>
        <label for="xGrid">
          <input id="xGrid" type="checkbox" ng-model="options.grid.x">X grid
        </label>
      </div>

    '''

    js: '''
      function($scope) {
        $scope.data = data;
        $scope.options = options;
      }
    '''

    }

